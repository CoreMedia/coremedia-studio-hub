<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="ehcache.xsd"
         updateCheck="true" monitoring="autodetect" dynamicConfig="true" name="cacheManagerShutterstock">

  <cache name="shutterstockCategoriesCache"
         maxEntriesLocalHeap="1000"
         eternal="false"
         diskSpoolBufferSizeMB="20"
         timeToIdleSeconds="300"
         timeToLiveSeconds="600"
         memoryStoreEvictionPolicy="LFU"
         transactionalMode="off"/>

  <cache name="shutterstockPicturesByCategoryCache"
         maxEntriesLocalHeap="1000"
         eternal="false"
         diskSpoolBufferSizeMB="20"
         timeToIdleSeconds="300"
         timeToLiveSeconds="600"
         memoryStoreEvictionPolicy="LFU"
         transactionalMode="off"/>

  <cache name="shutterstockPictureCache"
         maxEntriesLocalHeap="1000"
         eternal="false"
         diskSpoolBufferSizeMB="20"
         timeToIdleSeconds="300"
         timeToLiveSeconds="600"
         memoryStoreEvictionPolicy="LFU"
         transactionalMode="off"/>

  <cache name="shutterstockCategoryCache"
         maxEntriesLocalHeap="1000"
         eternal="false"
         diskSpoolBufferSizeMB="20"
         timeToIdleSeconds="300"
         timeToLiveSeconds="600"
         memoryStoreEvictionPolicy="LFU"
         transactionalMode="off"/>

  <cache name="shutterstockPicturesByCategorySearch"
         maxEntriesLocalHeap="1000"
         eternal="false"
         diskSpoolBufferSizeMB="20"
         timeToIdleSeconds="300"
         timeToLiveSeconds="600"
         memoryStoreEvictionPolicy="LFU"
         transactionalMode="off"/>
</ehcache>
