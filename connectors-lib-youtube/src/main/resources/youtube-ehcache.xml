<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="ehcache.xsd"
         updateCheck="true" monitoring="autodetect" dynamicConfig="true" name="cacheManagerYouTube">

  <cache name="youTubePlayListByUserCache"
         maxEntriesLocalHeap="1000"
         eternal="false"
         diskSpoolBufferSizeMB="20"
         timeToIdleSeconds="300"
         timeToLiveSeconds="600"
         memoryStoreEvictionPolicy="LFU"
         transactionalMode="off"/>

  <cache name="youTubePlayListByChannelCache"
         maxEntriesLocalHeap="1000"
         eternal="false"
         diskSpoolBufferSizeMB="20"
         timeToIdleSeconds="300"
         timeToLiveSeconds="600"
         memoryStoreEvictionPolicy="LFU"
         transactionalMode="off"/>

  <cache name="youTubeVideoCache"
         maxEntriesLocalHeap="1000"
         eternal="false"
         diskSpoolBufferSizeMB="20"
         timeToIdleSeconds="300"
         timeToLiveSeconds="600"
         memoryStoreEvictionPolicy="LFU"
         transactionalMode="off"/>

  <cache name="youTubeVideosCache"
         maxEntriesLocalHeap="1000"
         eternal="false"
         diskSpoolBufferSizeMB="20"
         timeToIdleSeconds="300"
         timeToLiveSeconds="600"
         memoryStoreEvictionPolicy="LFU"
         transactionalMode="off"/>

  <cache name="youTubePlaylistItemsCache"
         maxEntriesLocalHeap="1000"
         eternal="false"
         diskSpoolBufferSizeMB="20"
         timeToIdleSeconds="300"
         timeToLiveSeconds="600"
         memoryStoreEvictionPolicy="LFU"
         transactionalMode="off"/>

  <cache name="youTubeSearchCache"
         maxEntriesLocalHeap="1000"
         eternal="false"
         diskSpoolBufferSizeMB="20"
         timeToIdleSeconds="300"
         timeToLiveSeconds="600"
         memoryStoreEvictionPolicy="LFU"
         transactionalMode="off"/>

</ehcache>
